[sub_resource type="Shader" id=2]
code = "shader_type canvas_item;

uniform vec2 world_position;
uniform float light_strength;
uniform vec2 light_position;


void fragment(){
	vec2 screen_size = 0.5 / SCREEN_PIXEL_SIZE;
	vec2 frag_offset =  (FRAGCOORD.xy);

	vec2 offset = world_position - frag_offset - light_position;
	float light_distance_sq = offset.x * offset.x + offset.y * offset.y;
	float light_strength_sq = light_strength * light_strength;
	float lightness = clamp((light_strength_sq - light_distance_sq) * 2.0, 0.0, 1.0);
	COLOR = texture(TEXTURE, UV) * vec4(vec3(lightness), 1.0);
	// COLOR = vec4(frag_offset * SCREEN_PIXEL_SIZE, 0.0, 1.0);
}"

